<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>Motormoth.Chrono</AssemblyName>
		<RootNamespace>Motormoth.Chrono</RootNamespace>
		<Product>Motormoth .NET Chrono</Product>
		<Description>Date and time types for .NET</Description>
		<Authors>Aleksandr Melnik</Authors>
		<Company>Aleksandr Melnik</Company>
		<Copyright>2021 Aleksandr Melnik</Copyright>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<RepositoryUrl>https://github.com/motormoth/dotnet-chrono.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Version>1.0.0-rc1</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
	
</Project>